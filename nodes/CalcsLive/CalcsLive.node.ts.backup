// Metadata cache for article data - reduces API calls and improves performance
// Cache is keyed by articleId and stores the full metadata response
const metadataCache: Record<string, any> = {};

export class CalcsLive implements INodeType {
	methods = {
		loadOptions: {
			
			// Load available input PQs from validate endpoint with caching
			async getInputPQs(this: ILoadOptionsFunctions): Promise<INodePropertyOptions[]> {
				const articleId = this.getCurrentNodeParameter('articleId') as string;
				
				console.log('üÜî getInputPQs called for articleId:', articleId);
				
				if (!articleId) {
					return [{ name: 'Enter Article ID first', value: '' }];
				}
				
				// Check cache first
				let metadata = metadataCache[articleId];
				
				if (!metadata) {
					console.log('üíæ Cache miss - fetching metadata for:', articleId);
					try {
						const credentials = await this.getCredentials('calcsLiveApi');
						const baseUrl = credentials.baseUrl || 'https://www.calcs.live';
						
						const response = await this.helpers.httpRequest({
							method: 'GET',
							url: `${baseUrl}/api/n8n/validate?articleId=${articleId}&apiKey=${credentials.apiKey}`,
						});
						
						console.log('‚úÖ Validate API response received for:', articleId);
						
						if (response.success && response.metadata) {
							metadata = response.metadata;
							metadataCache[articleId] = metadata;
							console.log('üíæ Cached metadata for:', articleId);
						} else {
							return [{ name: 'No input PQs found', value: '' }];
						}
					} catch (error: any) {
						console.log('‚ùå API error:', error.message);
						return [{ name: `Error: ${error.message || 'Failed to load'}`, value: '' }];
					}
				} else {
					console.log('‚ö° Cache hit - using cached metadata for:', articleId);
				}
				
				if (!metadata?.inputPQs) {
					return [{ name: 'No input PQs found', value: '' }];
				}
				
				// Return clean symbol names as values, metadata is cached for unit lookup
				const options = metadata.inputPQs.map((pq: any) => ({
					name: `${pq.symbol} - ${pq.description || pq.symbol}`, // Display: "D - Distance" 
					value: pq.symbol, // Clean symbol value: "D", "t", "s"
				}));
				
				return options;
			},

			
			// Load available output PQs from validate endpoint with caching  
			async getOutputPQs(this: ILoadOptionsFunctions): Promise<INodePropertyOptions[]> {
				const articleId = this.getCurrentNodeParameter('articleId') as string;
				
				if (!articleId) {
					return [{ name: 'Enter Article ID first', value: '' }];
				}
				
				// Check cache first
				let metadata = metadataCache[articleId];
				
				if (!metadata) {
					console.log('üíæ Cache miss in getOutputPQs - fetching metadata for:', articleId);
					try {
						const credentials = await this.getCredentials('calcsLiveApi');
						const baseUrl = credentials.baseUrl || 'https://www.calcs.live';
						const response = await this.helpers.httpRequest({
							method: 'GET',
							url: `${baseUrl}/api/n8n/validate?articleId=${articleId}&apiKey=${credentials.apiKey}`,
						});
						
						if (response.success && response.metadata) {
							metadata = response.metadata;
							metadataCache[articleId] = metadata;
							console.log('üíæ Cached metadata for:', articleId);
						} else {
							return [{ name: 'No output PQs found', value: '' }];
						}
					} catch (error: any) {
						return [{ name: `Error: ${error.message || 'Failed to load'}`, value: '' }];
					}
				} else {
					console.log('‚ö° Cache hit in getOutputPQs - using cached metadata for:', articleId);
				}
				
				if (!metadata?.outputPQs) {
					return [{ name: 'No output PQs found', value: '' }];
				}
				
				const options = metadata.outputPQs.map((pq: any) => ({
					name: pq.symbol, // Just the symbol (v, s, result, etc.)
					value: JSON.stringify({
						symbol: pq.symbol,
						unit: pq.unit,
						categoryId: pq.categoryId
					}), // Embed metadata in value for later use
				}));
				
				return options;
			},

			// Load unit options for a specific symbol - dynamic per PQ
			async getUnitsForSymbol(this: ILoadOptionsFunctions): Promise<INodePropertyOptions[]> {
				console.log('\nüîç === getUnitsForSymbol called ===');
				console.log('‚è∞ Timestamp:', new Date().toISOString());
				
				const articleId = this.getCurrentNodeParameter('articleId') as string;
				
				if (!articleId) {
					console.log('‚ùå No articleId - returning guidance message');
					return [{ name: 'Enter Article ID first', value: '' }];
				}
				
				// Get all current parameters for debugging
				const allParams = this.getCurrentNodeParameters();
				console.log('üîç All current parameters:', JSON.stringify(allParams, null, 2));
				
				// Smart symbol detection - try multiple approaches
				let symbolParam: string | undefined;
				let actualSymbol: string | undefined;
				
				// Approach 1: Try direct parameter access
				try {
					symbolParam = this.getCurrentNodeParameter('symbol') as string;
					console.log('üìç Method 1 - Direct symbol parameter:', symbolParam);
				} catch (e) {
					console.log('üìç Method 1 - Direct symbol parameter failed:', e);
				}
				
				// Approach 2: Extract from current node context by analyzing all configured PQs
				if (!symbolParam || symbolParam === '') {
					console.log('üìç Method 2 - Analyzing configured PQ rows...');
					
					if (allParams && typeof allParams === 'object') {
						const inputPQs = (allParams as any).inputPQs;
						const outputPQs = (allParams as any).outputPQs;
						
						// Collect all configured symbol-unit pairs
						const symbolUnitPairs: Array<{
							symbol: string;
							unit: string | undefined;
							type: 'input' | 'output';
							index: number;
						}> = [];
						
						// Process input PQs
						if (inputPQs && inputPQs.pq && Array.isArray(inputPQs.pq)) {
							inputPQs.pq.forEach((item: any, index: number) => {
								if (item.symbol && item.symbol.trim() !== '') {
									symbolUnitPairs.push({
										symbol: item.symbol,
										unit: item.unit || undefined,
										type: 'input',
										index
									});
								}
							});
						}
						
						// Process output PQs
						if (outputPQs && outputPQs.pq && Array.isArray(outputPQs.pq)) {
							outputPQs.pq.forEach((item: any, index: number) => {
								if (item.symbol && item.symbol.trim() !== '') {
									symbolUnitPairs.push({
										symbol: item.symbol,
										unit: item.unit || undefined,
										type: 'output',
										index
									});
								}
							});
						}
						
						console.log('üìã Found symbol-unit pairs:', symbolUnitPairs);
						
						// Smart selection: find rows with symbols but no units (being edited)
						const incompleteRows = symbolUnitPairs.filter(pair => 
							pair.symbol && (!pair.unit || pair.unit === '')
						);
						
						console.log('üéØ Incomplete rows (likely being edited):', incompleteRows);
						
						if (incompleteRows.length === 1) {
							symbolParam = incompleteRows[0].symbol;
							console.log('‚úÖ Smart detection: Using symbol from incomplete row:', symbolParam);
						} else if (incompleteRows.length > 1) {
							// Multiple incomplete - use last one
							symbolParam = incompleteRows[incompleteRows.length - 1].symbol;
							console.log('‚ö†Ô∏è Multiple incomplete rows - using last:', symbolParam);
						} else if (symbolUnitPairs.length > 0) {
							// All rows complete - use last configured symbol
							symbolParam = symbolUnitPairs[symbolUnitPairs.length - 1].symbol;
							console.log('‚ö†Ô∏è All rows complete - using last configured:', symbolParam);
						}
					}
				}
				
				// If still no symbol found, return helpful message
				if (!symbolParam || symbolParam === '' || symbolParam === 'undefined' || symbolParam === 'null') {
					console.log('‚ùå No symbol detected - returning guidance message');
					return [{ 
						name: '‚Üê Select a symbol first', 
						value: '',
						description: 'Choose a physical quantity symbol before selecting units'
					}];
				}
				
				// Parse symbol if it's embedded JSON (from loadOptions)
				try {
					const parsed = JSON.parse(symbolParam);
					actualSymbol = parsed.symbol;
					console.log('‚úÖ Parsed embedded symbol:', actualSymbol);
				} catch (e) {
					actualSymbol = symbolParam;
					console.log('‚úÖ Using plain symbol:', actualSymbol);
				}
				
				// Get or fetch metadata
				let metadata = metadataCache[articleId];
				
				if (!metadata) {
					console.log('üíæ Cache miss - fetching metadata for:', articleId);
					try {
						const credentials = await this.getCredentials('calcsLiveApi');
						const baseUrl = credentials.baseUrl || 'https://www.calcs.live';
						const response = await this.helpers.httpRequest({
							method: 'GET',
							url: `${baseUrl}/api/n8n/validate?articleId=${articleId}&apiKey=${credentials.apiKey}`,
						});
						
						console.log('üåê API Response received:', {
							success: response.success,
							hasMetadata: !!response.metadata,
							metadataKeys: response.metadata ? Object.keys(response.metadata) : []
						});
						
						if (response.success && response.metadata) {
							metadata = response.metadata;
							metadataCache[articleId] = metadata;
							console.log('üíæ Cached metadata successfully');
							
							// Debug the metadata structure
							console.log('üìã Metadata structure:', {
								hasInputPQs: !!metadata.inputPQs,
								inputPQsCount: metadata.inputPQs?.length || 0,
								hasOutputPQs: !!metadata.outputPQs,
								outputPQsCount: metadata.outputPQs?.length || 0,
								hasAvailableUnits: !!metadata.availableUnits,
								availableUnitsKeys: metadata.availableUnits ? Object.keys(metadata.availableUnits) : []
							});
						} else {
							console.log('‚ùå Invalid API response:', response);
							return [{ name: 'Failed to load article metadata', value: '' }];
						}
					} catch (error: any) {
						console.log('‚ùå API Error:', error);
						return [{ name: `API Error: ${error.message}`, value: '' }];
					}
				} else {
					console.log('‚ö° Cache hit - using cached metadata');
				}
				
				// Find PQ data for the symbol
				const allPQs = [...(metadata.inputPQs || []), ...(metadata.outputPQs || [])];
				console.log('üìã Available PQ symbols:', allPQs.map((pq: any) => pq.symbol));
				console.log('üîç Looking for symbol:', actualSymbol);
				
				const pqData = allPQs.find((pq: any) => pq.symbol === actualSymbol);
				
				if (!pqData) {
					console.log('‚ùå Symbol not found in current calc');
					const availableSymbols = allPQs.map((pq: any) => pq.symbol).join(', ');
					return [
						{ name: `‚ö†Ô∏è Symbol "${actualSymbol}" not found`, value: '' },
						{ name: `Available: ${availableSymbols}`, value: '' },
						{ name: 'üí° Remove this row and add a new one', value: '' }
					];
				}
				
				console.log('‚úÖ Found PQ data:', {
					symbol: pqData.symbol,
					unit: pqData.unit,
					categoryId: pqData.categoryId
				});
				
				// Get available units for this category
				const categoryId = pqData.categoryId;
				const availableUnits = metadata.availableUnits?.[categoryId];
				
				console.log('üè∑Ô∏è Category ID:', categoryId);
				console.log('üìè Available units for category:', availableUnits);
				console.log('üìè Full availableUnits structure:', metadata.availableUnits);
				
				if (!availableUnits || !Array.isArray(availableUnits) || availableUnits.length === 0) {
					console.log('‚ùå No units available for category:', categoryId);
					return [{ 
						name: `No units available for category ${categoryId}`, 
						value: pqData.unit || '' 
					}];
				}
				
				// Build options with original unit first
				const originalUnit = pqData.unit;
				const options = [];
				
				// Add original unit first if it exists in available units
				if (originalUnit && availableUnits.includes(originalUnit)) {
					options.push({
						name: `${originalUnit} (default)`,
						value: originalUnit,
					});
				}
				
				// Add all other units (excluding original to avoid duplicates)
				availableUnits.forEach((unit: string) => {
					if (unit !== originalUnit) {
						options.push({
							name: unit,
							value: unit,
						});
					}
				});
				
				console.log('‚úÖ Built unit options:', options.length, 'units');
				console.log('üì§ Returning options:', options.map(o => o.name));
				
				return options;
			},
		},
	};

