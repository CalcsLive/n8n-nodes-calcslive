{
  "name": "@calcslive/n8n-nodes-calcslive",
  "version": "0.1.4",
  "description": "Plug-and-play unit conversion and unit-aware multi-step calculations for n8n workflows. Transform your automation with automatic handling of 540+ units across 64+ categories.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "calcslive",
    "calculator",
    "physics",
    "units",
    "conversion",
    "engineering",
    "mathematics",
    "automation",
    "api",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "https://www.calcs.live",
  "author": "CalcsLive <don.wen@calcs.live> (https://www.calcs.live)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calcslive/n8n-nodes-calcslive.git"
  },
  "bugs": {
    "url": "https://github.com/calcslive/n8n-nodes-calcslive/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://www.calcs.live/pricing"
  },
  "engines": {
    "node": ">=18.10",
    "n8n": ">=0.200.0"
  },
  "main": "index.js",
  "scripts": {
    "b": "npm run build",
    "build": "npm run clean && tsc && gulp build:icons",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --fix",
    "test": "echo \"Tests will be added in future versions\" && exit 0",
    "typecheck": "tsc --noEmit",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build && npm run lint && npm run typecheck",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CalcsLiveApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CalcsLive/CalcsLive.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^8.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
